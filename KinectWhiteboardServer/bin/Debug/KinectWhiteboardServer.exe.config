<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="addr" value="net.tcp://192.168.1.3:22222/chatservice"/>
  </appSettings>
  <system.serviceModel>
    <services>
      <service name="KinectWhiteboardServer.ChatService">
        <endpoint address="" binding="netTcpBinding" bindingConfiguration="DuplexBinding" contract="KinectWhiteboardServer.IChat"/>
      </service>
    </services>
    <bindings>
      <netTcpBinding>
        <binding name="DuplexBinding" maxConnections="10000">
          <reliableSession enabled="true"/>
          <security mode="None"/>
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/>
  </startup>
</configuration>
